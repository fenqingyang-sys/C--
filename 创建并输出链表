# C-
C语言函数
# include <stdio.h>
# include <stdlib.h>
typedef struct node
{
	int data;
	struct node *next;
}NODE;
void main ()
{
	int a[5]={1,2,3,4,5};
	int i;
	NODE *p,*q=NULL,*h=NULL;
	for(i=0;i<5;i++)
	{
		p=(NODE*)malloc(sizeof(struct node));
		p->data=a[i];
		if(h==NULL)
		{
			h=p;
		}
		else
		{
			q->next=p;
		}
		q=p;
	}
	if(q!=NULL)
	{
		q->next=NULL;
	}
	while(p!=NULL)
	{
		printf("%d  ",h->data);
		h=h->next;
	}
		
}
 
