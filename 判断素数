# C-
C语言函数
#include<stdio.h>
int sushu(int i)
{

int k;int ret=1;
for(k=2;k<i;k++)
	{
		if(i%k==0)
		{
			ret=0;
			break;
		}
		return ret;
	}
	

}

int main()
{   
    int x; 
    scanf("%d",&x);
    if(x>0)
    {	
		if(sushu(x))
		{
			printf("是素数"); 
		}
	 } 
	 else
	 printf("不是素数"); 
	    
	     	
	
}
